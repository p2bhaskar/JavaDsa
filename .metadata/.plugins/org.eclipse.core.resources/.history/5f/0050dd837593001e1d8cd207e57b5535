package arrays;

public class Arrays_01 {

	public static void main(String[] args) {
		int[] arr = {23,-1,45,32,89,9,7};
		int n= arr.length;
		
		for(int i=0;i<=arr.length/2-1;i++) {
			
			int temp = arr[i];
			arr[i] = arr[n-1-i];
			arr[n-1-i] = arr[i];
		}
		
		for(int i:arr) {
			System.out.print(i+" ");
		}
	}

}
