package pckg_01;

public class ReverseArr {

	public static void main(String[] args)
	{
		int[] arr = {34,67,21,78};
		
		for(int i=0;i<(arr.length-1)/2;i++){
			
			int temp = arr[i];
			 arr[i] = arr[arr.length-1-i];
			 arr[arr.length-1-i] = temp;
		}
		for(int i:arr) {
			System.out.print(i+" ");
		}

	}

}
